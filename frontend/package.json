{
  "name": "react-itinerary-planner",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "history": "^5.1.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "dev-frontend": "REACT_ENV=TEST && react-scripts --openssl-legacy-provider start",
    "start": "npm run clientStart",
    "dev": "concurrently \"cd backend && npm run start\" \"npm run clientStart\"",
    "devMac": "concurrently \"cd backend && npm run start\" \"npm run clientStartMac\"",
    "clientStart": "react-scripts start",
    "clientStartMac": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts build",
    "buildMac": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-useless-concat": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
